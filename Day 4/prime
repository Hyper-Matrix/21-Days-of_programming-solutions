
import java.util.*
fun prime(a:Int,b:Int){
    var m = a
    while(m<b){
        var flag=false
        for(i in 2 .. a/2){
            if(m%i==0){
                flag=true
                break
            }

        }
        if(flag==false) {
            print("${m}\n")}

         m++

    }
}

fun main(args:Array<String>){

var sc = Scanner(System.`in`)
    print("Enter 1st Number")
    var a = sc.nextInt()
    print("No last number")
    var b = sc.nextInt()
    prime(a,b)

}

